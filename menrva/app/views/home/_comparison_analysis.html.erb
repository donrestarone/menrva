<div class="alert alert-success" role="alert">
	<h4 class="alert-heading">Successfully Analyzed!</h4>
	<p><%= comparison %></p>
	<hr>
	<ul class="list-group">
		<% occurances.each do |occurance| %>
			<li class="list-group-item d-flex justify-content-between align-items-center">
				<%= "'#{occurance[:word]}'" %>
				<span class="badge badge-primary badge-pill"><%= "#{occurance[:frequency]}" %></span>
			</li>
		<% end %> 
	</ul>
</div>